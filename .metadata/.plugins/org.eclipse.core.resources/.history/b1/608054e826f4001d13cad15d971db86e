package Tarea1;

public class ColaCharacter {
	private char lett;
	private String palab;
	private PilaLinkedExtend pila;
	
	public ColaCharacter(char lett, String palab) {
		this.lett = lett;
		this.palab = palab;
		this.pila = new PilaLinkedExtend();
	}
	
	public void cargar() {
		while(!this.pila.estaVacia()) {
			
		}
	}
}
